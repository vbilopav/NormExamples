@page
@model NormExample.Pages.RazorPageModel1
@{
    Layout = "_Layout.cshtml";
}

<h1>Razor Page Norm data access example</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Username</th>
            <th>Roles</th>
        </tr>
    </thead>
    <tbody>
        @await foreach (var user in Model.Service.GetUsersAsync())
        {
            <tr>
                <td>@user.id</td>
                <td>@user.userName</td>
                <td>@user.email</td>
            </tr>
        }
    </tbody>
</table>
